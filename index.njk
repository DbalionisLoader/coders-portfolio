 {# project-wrapper #}
      <div class="project-wrapper card-wrapper-mobile mt-4">   
        {%- for post in collections.posts | reverse -%}
            <div class="project-box">
            <a href="{{ post.data.link }}">
              <img class="img-fluid" src="{{ post.data.imageUrl }}"  alt="{{ post.imageAlt }}">
            </a>
              <div class="tag-line-wrapper">
                  {# tag in local var, post.data.tech is a array #}
                    {% for tag in post.data.tech %}
               <div class="tag">{{ tag }}</div>
                  {% endfor %}
              </div>
              <div class="box-text-wrapper">
                <a href="helloworld.html" aria-label="Link a view a hello world webpage">
                  <h2>{{ post.data.title }}</h2>
                 
                  <p>{{ post.content | safe }}</p>
              </div>
              <div class="link-opacity-10-hover readmore-link hover:underline">Read more </div>
              </a> 
        </div>
        {%- endfor -%}
      </div> 
      </section>
      <article class="container-fluid jsArticle-wrapper mt-5">
   {#    <div> <img src = "../images/JamProgram3.svg" style="height: 140px"  alt="My Happy SVG"/></div> #}
      <div class="portf-header text-start">
      <h1>My Sandbox</h1>
      <p>A collection of coding excercises showcasing my JS and codings skills</p>
      </div>
    </div>
      </article>
        <div id="contact-form" class="container p-4 contact-wrapper">
