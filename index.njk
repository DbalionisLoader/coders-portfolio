 {# project-wrapper #}
      <div class="project-wrapper card-wrapper-mobile mt-4">   
        {%- for post in collections.posts | reverse -%}
            <div class="project-box">
            <a href="{{ post.data.link }}" aria-label="{{ post.data.alabel}}">
              <img class="img-fluid" src="{{ post.data.imageUrl }}" loading="lazy"  width="445" height="312" alt="{{ post.imageAlt }}">
            </a>
              <div class="tag-line-wrapper">
                  {# tag in local var, post.data.tech is a array #}
                    {% for tag in post.data.tech %}
               <div class="tag">{{ tag }}</div>
                  {% endfor %}
              </div>
              <div class="box-text-wrapper">
                <a href="{{ post.data.link }}" aria-label="Link a view a hello world webpage">
                  <h2>{{ post.data.name }}</h2>
                 
                  <p>{{ post.data.shortDesc }}</p>
              </div>
              <div class="link-opacity-10-hover readmore-link hover:underline ">Read more </div>
              </a> 
        </div>
        {%- endfor -%}
